{
  "name": "phpbcc/phpbcc",
  "type": "library",
  "description": "A tool that provides visibility on the authors of uncovered code.",
  "license": "BSD3-clause",
  "keywords": ["git", "php", "blame", "code", "coverage", "quality"],
  "authors": [
    {
      "name": "Chris Halbert",
      "email": "christopher.halbert@gmail.com"
    }
  ],
  "require": {
    "php": "7.*",
    "symfony/console": "3.*",
    "chrishalbert/git": "1.*"
  },
  "require-dev": {
    "phing/phing": "2.*",
    "phpunit/phpunit": "4.*",
    "squizlabs/php_codesniffer": "2.*",
    "chadicus/coding-standard": "~1.0"
  },
  "autoload": {
    "psr-4": {
      "ChrisHalbert\\PhpBCC\\": "src"
    }
  },
  "bin": [
    "phpbcc"
  ]
}
